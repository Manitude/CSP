<div id="create_new_session_ms" style="padding-left:5px; padding-right: 5px;">
  <div id="bottom_part_of_facebox" style="float: left; position: relative; width: 100%;">
    <% if @session_details[:coaches].any? %>
      <div id="available_coaches_container" style="margin-left:0px; margin-right:-10px;">
        Included Coaches:
        <%= render(:partial => 'coach_lists', :locals => {:coaches => @session_details[:coaches], :excluded_coaches => @session_details[:excluded_coaches]} )%>
      </div>
      <div class="right_container lotus_extra_right">
        <div id="extra_session_name" style="padding-bottom: 10px;">
          Session Name:
          <div>
            <%= text_field_tag 'session_name_txt', @session_details[:extra_session].name %>
          </div>
        </div>
        <div id="button_container">
          <input type = "button" id = "edit_extra_session_btn_lotus" class = "button" value = "SAVE" onclick ="updateExtraSessionReflex('<%= @session_details[:extra_session].id %>',this)"/>
        </div>
      </div>
    <% else %>
      There are no active coaches qualified to teach this language.
    <% end %>
  </div>
</div>
