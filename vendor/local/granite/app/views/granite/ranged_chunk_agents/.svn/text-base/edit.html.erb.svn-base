<h2>Edit Ranged Chunk Agent</h2>
<% ranged_chunk_id = @ranged_chunk_agent ? @ranged_chunk_agent.id : nil %>
<%= form_for(:ranged_chunk_agent, :url => {:action => :update, :id => ranged_chunk_id}, :html => { :method => :put }) do |f| %>
  <%= render :partial=>'granite/ranged_chunk_agents/form', :object => f %>
  <p><%= submit_tag 'Save' %></p>
<% end %>

<p><%= link_to 'Back', :action => :index %></p>