}

div#community_profile{
  font-size:11px;
}

span.subheading {
  color:#FFA519;
  font-size:13px;
  font-weight:lighter;
}

#profile{
  font-size: 11px;
}

#extranet #content div.outlined_table table th, #extranet #content div.outlined_table table td {
  height:30px;
  padding:4px;
  vertical-align:middle;
}

#extranet #content div.outlined_table {
  float:left;
  padding-left:0px;
  width:100%;
}

#extranet #content div.outlined_table table{
  border-collapse: collapse;
  padding-top:0px;
  width:auto;
}

p.italics {
  font-style: italic;
  padding-left: 21px;
}

.avatar img {
 width:55px;
}

div.path_score_details span.lighter_label{
