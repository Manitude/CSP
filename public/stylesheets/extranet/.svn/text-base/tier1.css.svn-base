}

.studio_info div.wide_label_row{
  width: 340px
}

#community_profile .public{
  height:auto;
}

.learner_studio_history_link{
  float:left;
  padding-top:20px;
}

.padding_top_30{
  padding-top: 20px
}

.studio_info div.col-1,
.studio_info div.col-2,
.studio_info div.col-3 {
