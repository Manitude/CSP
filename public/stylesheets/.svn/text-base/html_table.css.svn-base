div.tableContainer table {
}

/* set table header to a fixed position. WinIE 6.x only                                       */
/* In WinIE 6.x, any element with a position property set to relative and is a child of       */
/* an element that has an overflow property set, the relative value translates into fixed.    */
/* Ex: parent element DIV with a class of tableContainer has an overflow property set to auto */
thead.fixedHeader tr {
  position: relative
}

/* set THEAD element to have block level attributes. All other non-IE browsers            */
/* this enables overflow to work on TBODY element. All other non-IE, non-Mozilla browsers */
thead.fixedHeader tr {
  display: block
}

.table_list th {
    border: 1px solid #CCCCCC;
    white-space: normal;
}

/* define the table content to be scrollable                                              */
/* set TBODY element to have block level attributes. All other non-IE browsers            */
/* this enables overflow to work on TBODY element. All other non-IE, non-Mozilla browsers */
/* induced side effect is that child TDs no longer accept width: auto                     */
tbody.scrollContent {
    display: block;
    max-height: 288px !important;
    overflow: auto;
    width: 100%
}

tbody.scrollContent_mod {
    display: block;
    max-height: 262px;
    overflow: auto;
    width: 100%
}

tbody > tr > td{
    word-wrap:break-word;
}
  .table_list_substitution{
    margin: 25px auto;
    width: 100%;
    border-bottom: 1px solid #CCCCCC;
    border-top: 1px solid #CCCCCC;
  }
  .table_list_substitution td, .table_list_substitution th {
    padding: 4px 2px;
    text-align: center;
  }
  .table_list_substitution td {
    text-align: left;
    font-size:11px;
  }
  .select_class {
    font-size: 9px;
  }
  .table_list_substitution th {
    text-transform: none;
    font-size: 10px;
    background: none repeat scroll 0 0 #F0F0F0;
    color: #666666;
    font-weight: bold;
  }
  .sub_center {
    padding-left:10px !important;
  }
  .coach_requesting_column {
    max-width: 50px;
    word-wrap: break-word;
  }
  .ui-tooltip {
    max-width: 540px;
    min-width: 380px;
  }
  .table_list_substitution td.left-border, .table_list_substitution th.left-border {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: -moz-use-text-color -moz-use-text-color -moz-use-text-color #CCCCCC;
    border-image: none;
    border-style: none none none solid;
    border-width: medium medium medium 1px;
  }
  .table_list_substitution td.left-right-border, .table_list_substitution th.left-right-border {
    -moz-border-bottom-colors: none;
    -moz-border-left-colors: none;
    -moz-border-right-colors: none;
    -moz-border-top-colors: none;
    border-color: -moz-use-text-color #CCCCCC;
    border-image: none;
    border-style: none solid;
    border-width: medium 1px;
  }

