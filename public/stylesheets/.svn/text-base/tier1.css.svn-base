html, body {
  height: 100%;
}

#header {overflow:hidden !important; height:auto;}
#container { clear: left; position: static;}
#extranet #content {width: 800px;}
a { color: #A9160E; }

a.column-sort-link { color: #A9160E; }

.wrapper { margin: 0 auto; width: 90%; text-align: left; }

h1 { font: 1.8em/1.8em Georgia, Times, "Times New Roman", serif; color: #fff; }

h3 {
  font-weight: bold;
  margin-bottom: 4px;
  text-shadow: 0.1em 0.1em 0.3em rgba(50, 50, 50, 0.25);
  color: #444;
}
h4 { font-style: italic; }

p { margin-bottom: 10px; }

p.flash {
  background: #fff;
  border: 1px solid red;
  color: red;
  padding: 5px;
  margin-top: 15px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -moz-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.15);
  -webkit-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.15);
  box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.15);
  text-shadow: 0.15em 0.15em 0.1em rgba(20, 20, 20, 0.15);
}

p.success {
  border-color: #6EC563;
  color: #2E9513;
}

.notice { color: #666; }
.notice strong { font-weight: bold; }

#nav_main { position: absolute; bottom: 0; }
#nav_main li { display: block; float: left; margin-right: 5px; }
#nav_main li a {
  display: block;
  padding: 5px 10px;
  background: #A9160E;
  text-decoration: none;
  color: #eee;
  -moz-border-radius-topleft: 4px;
  -moz-border-radius-topright: 4px;
  -webkit-border-top-left-radius: 4px;
  -webkit-border-top-right-radius: 4px;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  border-left: 1px solid #700000;
  border-top: 1px solid #700000;
  border-right: 1px solid #700000;
  text-shadow: 0.15em 0.15em 0.2em rgba(50, 50, 50, 0.15);
  -moz-box-shadow: 2px -1px 4px rgba(40, 60, 40, 0.1);
  -webkit-box-shadow: 2px -1px 4px rgba(40, 60, 40, 0.1);
  box-shadow: 2px -1px 4px rgba(40, 60, 40, 0.1);
}
#nav_main li a.active { background: #f7f3e6; color: #A9160E; }
#nav_main li em { text-decoration: underline; }

#sub_nav {  padding: 10px 0; }
  #sub_nav ul { width: 90%; margin: 0 auto; }
  #sub_nav li { display: block; float: left; margin-right: 20px;  }
    #sub_nav li a.active { font-weight: bold;}
    .separator { clear: both; border-bottom: 1px solid #ddd; width: 90%; margin: 0 auto; padding-top: 10px;}

#logout {
  position: absolute;
  top: 0px;
  color: #fff;
  right: 0;
  background: #A9160E;
  padding: 0px 8px 5px 8px;
  margin-top: 0px;
  -moz-border-radius-bottomleft: 6px;
  -webkit-border-bottom-left-radius: 6px;
  border-bottom-left-radius: 6px;
}
#logout strong { font-weight: bold; }
#logout a {color: #fff;}


.pages_missing { float: left;}
table {
  text-align: left;
  border: 1px solid #bbb;
  margin: 0 0 12px 0;
  width: 100%;
  background-color: #fff;
  border-collapse: separate;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -moz-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
  -webkit-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
  box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
}
table thead { background-color: #f0ecea; }
table caption { font: 2em/2em Georgia, Times, "Times New Roman", serif; color: #333; margin: 10px 0 5px 0; }
table td, table th { padding: 4px 3px 3px 3px; width:auto;}
table#product_rights td, table#product_rights th {font-size:10px;}
.learner-table td, th {border: 1px solid #CCCCCC; font-size:100%;}
table td.disabled { background: #e0e0e0; }
table td.default { background: #99ccff; }
table th { font-weight: bold; }
table td { border-top: 1px solid #ddd; }
#facebox td { border-top: 0 none; text-align: left; }
tbody tr:nth-child(even) { background-color: #eee; }

th { font-weight: bold; white-space: nowrap; }

#unclaimed_product_rights, #end_user_license_list, #product_usage_logs {
  clear: both;
}

#end_user_license_list strong {
  font-weight: bold;
}

.pages {
  margin: 4px 0 5px 0;
  width: 80%;
  float: left;
}

.pages a {
  text-decoration: none;
  padding: 0 5px 2px 5px;
  border: 1px solid #bbb;
  margin: 0 3px 3px 0;
  display: block;
  float: left;
  background-color: #fff;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -moz-box-shadow: 1px 1px 3px rgba(20, 20, 20, 0.2);
  -webkit-box-shadow: 1px 1px 3px rgba(20, 20, 20, 0.2);
  box-shadow: 1px 1px 3px rgba(20, 20, 20, 0.2);
}

.pages a.active {
  background-color: #800;
  color: #fff;
}

.item_count {
  width: 20%;
  float: right;
  text-align: right;
  color: #777;
}

div.auto_complete {
  width: 450px;
  background: #fff;
}
div.auto_complete ul {
  border: 1px solid #888;
  margin: 0;
  padding: 0;
  width: 100%;
  list-style-type: none;
}
div.auto_complete ul li {
  margin: 0;
  padding: 3px;
}
div.auto_complete ul li.selected {
  background-color: #ff9;
}
div.auto_complete ul strong.highlight {
  color: #800;
  margin: 0;
  padding: 0;
}

#requirements { color: #999; margin-top: 20px; padding-top: 7px; border-top: 1px solid #ddd; }
  #requirements .help { text-align: right; float: right; }
  #requirements a { color: #888; }
  #requirements .interface { float: left; }
.description { display: block; color: #666; margin: 2px 0 5px 0; }
  .description strong { font-weight: bold; }

h2 strong { font-weight: bold; }
#license_details, #creation_account_details {
  margin-top: 10px;
  padding-top: 10px;
  border-top: 1px solid #bbb;
}
#license_details h2, #creation_account_details h2 {
  margin-top: 0;
}
.license_detail.miscellaneous dt, .creation_account_detail.miscellaneous dt {
  padding-left: 12px;
}
.license_detail.miscellaneous dd, .creation_account_detail.miscellaneous dd {
  padding: 1px 12px 2px 12px;
}

.license_detail dd, .creation_account_detail dd {
  overflow: hidden;
  text-overflow: ellipsis;
}

.license_detail dd:hover, .creation_account_detail dd:hover {
/*position: absolute;*/
  overflow: visible;
  background-color: rgba(247, 243, 230, 0.7);
  z-index: 2;
}

/* undo the hover effect on the miscellaneous & end user licenses details since it causes multiple dd's to overlap vertically */
.license_detail.miscellaneous dd:hover, .creation_account_detail.miscellaneous dd:hover, .creation_account_detail.end_user_licenses dd:hover {
  position: relative;
}

.license_detail .is_test, .creation_account_detail .is_test, .license_detail .is_demo, .creation_account_detail .is_demo, .license_detail .is_master {
  -moz-border-radius: 12px;
  -webkit-border-radius: 12px;
  border-radius: 12px;
  -moz-box-shadow: 2px 2px 5px rgba(20, 20, 20, 0.1);
  -webkit-box-shadow: 2px 2px 5px rgba(20, 20, 20, 0.1);
  box-shadow: 2px 2px 5px rgba(20, 20, 20, 0.1);
  border-width: 1px;
  border-style: solid;
}

.license_detail .is_master {
  background-color: #9130ff; /* for browsers that don't understand gradients */
  background: -moz-linear-gradient(top, rgba(145, 48, 255, 0.7), rgba(145, 48, 255, 0.4) 40%, rgba(145, 48, 255, 0.8));
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(145, 48, 255, 0.7)), to(rgba(145, 48, 255, 0.8)), color-stop(0.4, rgba(145, 48, 255, 0.4)));
  border-color: #7845D6;
}

.license_detail .is_test, .creation_account_detail .is_test {
  margin-top: 3px;
  background-color: #ffd117; /* for browsers that don't understand gradients */
  background: -moz-linear-gradient(top, rgba(255, 209, 23, 0.7), rgba(255, 209, 23, 0.4) 40%, rgba(255, 209, 23, 0.8));
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(255, 209, 23, 0.7)), to(rgba(255, 209, 23, 0.8)), color-stop(0.4, rgba(255, 209, 23, 0.4)));
  border-color: #ffbf17;
}

.license_detail .is_demo, .creation_account_detail .is_demo {
  margin-top: 3px;
  background-color: #ccc; /* for browsers that don't understand gradients */
  background: -moz-linear-gradient(top, rgba(200, 200, 200, 0.7), rgba(200, 200, 200, 0.3) 40%, rgba(200, 200, 200, 0.8));
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(200, 200, 200, 0.7)), to(rgba(200, 200, 200, 0.8)), color-stop(0.4, rgba(200, 200, 200, 0.3)));
  border-color: #bbb;
}

.submit { margin-left: 5px; }

dl#guid { margin: 0px 0px 6px 0px; }
dl#guid dt, dl#guid dd { display: inline-block; font-size: 14px}
dl#guid dt { font-weight: bold; }

.license_detail { float: left; width: 25%; margin-bottom: 14px; }
  .license_detail dt { font-weight: bold; }
  .license_detail dt, .license_detail dd {font-size:14px;}
  #license-info {
      -moz-border-radius: 6px 6px 6px 6px;
      -moz-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
      background: none repeat scroll 0 0 rgba(190, 185, 180, 0.3);
      padding: 5px 2%;
      width: 96%;
      float:left;
  }
.creation_account_detail { float:left; width: 15.8%; margin-bottom: 14px; padding-right: 2px; }
  .creation_account_detail dt { font-weight:bold; }

.not_usable { color: #999; }
.not_usable a { color: #F6ADC6; }
.information {
  width: 16px;
  padding: 4px 0 5px 4px;
}

#license_not_usable, #broken_token {
  margin-bottom: 10px;
  padding: 10px 10px 10px 25px;
  border: 1px solid #ccc;
  background: #fff url(/images/manage/error.png) no-repeat 5px .9em;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -moz-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.1);
  -webkit-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.1);
  box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.1);
}
#license_not_usable p { margin: 0; }

#main {
  float: left;
}
#main {padding:0; width:100%; font-size:80%;}
#sidebar {width:25%;float:right;}
#sidebar ul {
  position: relative;
  margin-left: 20px;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -moz-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
  -webkit-box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
  box-shadow: 2px 2px 7px rgba(20, 20, 20, 0.2);
}
#sidebar ul li {
  background: #fff;
  padding: 0;
  margin: 0;
  border-left: 1px solid #ddd;
  border-right: 1px solid #ddd;
  border-bottom: 1px solid #eee;
}
#sidebar ul li:first-child {
  -moz-border-radius-topleft: 4px;
  -moz-border-radius-topright: 4px;
  -webkit-border-top-left-radius: 4px;
  -webkit-border-top-right-radius: 4px;
  border-top-left-radius: 4px;
  border-top-right-radius: 4px;
  border-top: 1px solid #ddd;
}
#sidebar ul li:last-child {
  -moz-border-radius-bottomleft: 4px;
  -moz-border-radius-bottomright: 4px;
  -webkit-border-bottom-left-radius: 4px;
  -webkit-border-bottom-right-radius: 4px;
  border-bottom-left-radius: 4px;
  border-bottom-right-radius: 4px;
  border-bottom: 1px solid #ddd;
}
#sidebar ul li a {
  display: block;
  padding: 7px 5px 5px 20px;
  text-decoration: none;
  background: url(/images/manage/bullet_go.png) no-repeat 2px 55%;
}
#sidebar ul li:hover {
  background-color: #f3f3f3; /* for browsers that don't get gradients */
  background: -moz-linear-gradient(top center, #eee, #fff 35%, #efefef 65%, #dedede);
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#eee), to(#dedede), color-stop(0.35, #fff), color-stop(0.65, #efefef));
}

#authentication_token {
  width: 100%;
  font-size: 1em;
}

#extranet #content table {
  padding-top: 0px;
  width: 100%;
}

#product_rights {
  font-size: 0.95em;
  width: 960px;
}


#extranet #content table th {text-align: center}

#product_rights .guid, #end_user_license_list .guid, .extensions_table .guid {
  font-size: 1.15em;
  font-family: monospace;
}

#extensions, #content_ranges {
  margin-top: 12px;
}

.extension_fieldset p, .content_range_fieldset p {
  margin-top: 12px;
  margin-bottom: 0;
}

.extensions_table td {
  white-space: nowrap;
}

.min_max_unit, .row_actions {
  text-align: center;
}

.row_actions img {
  margin-right: 5px;
}

.extensions_tooltip, .content_ranges_tooltip {
  background-color: #dddddd;
  background: rgba(220,220,220,0.95);
  border: 1px solid #bbb;
  -moz-border-radius: 4px;
  -webkit-border-radius: 4px;
  border-radius: 4px;
  -moz-box-shadow: 6px 6px 6px rgba(60, 60, 60, 0.3);
  -webkit-box-shadow: 6px 6px 6px rgba(60, 60, 60, 0.3);
  box-shadow: 6px 6px 6px rgba(60, 60, 60, 0.3);
  padding: 8px;
}

/* used for the extensions boxbox */
#boxbox_content.boxbox_with_table {
  min-width: 80%;
  max-height: 70%;
  left: 10%;
  background-color: #dddddd;
  background: rgba(220, 220, 220, 0.85);
}

.product_right_extensions {
  color: #777;
  border-bottom: 1px dotted #777;
  text-decoration: none;
  cursor: pointer;
}

.expired {
  color: #999;
}

.burned td {
  color: #800000;
  text-decoration: line-through;
}

/* webkit likes to draw lines through images?  that's not text! :) */
.burned td.row_actions {
  text-decoration: none;
}

.content_range_indicator_cell {
  vertical-align: middle;
}

.content_range_indicator {
  margin: 4px 3px 3px 0;
  border: 1px solid #888;
  -moz-box-shadow: 2px 2px 5px rgba(10, 10, 10, 0.3);
  -webkit-box-shadow: 2px 2px 5px rgba(10, 10, 10, 0.3);
  box-shadow: 2px 2px 5px rgba(10, 10, 10, 0.3);
  background-color: #fff;  /* for browsers that don't get gradients */
  background: -moz-linear-gradient(center top, #ddd, #fff 35%, #ddd 75%, #bbb);
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(#ddd), to(#bbb), color-stop(0.35, #fff), color-stop(0.75, #ddd));
}

.content_range_indicator td, #extranet #content table.content_range_indicator td {
  padding: 9px 0 0 4px;
  border-top-width: 0;
  border-right: 1px solid #888;
  border-right: 1px solid rgba(150, 150, 150, 0.6);
}

.content_range_indicator td:last-child {
  border-right-width: 0;
}

.content_range_indicator .access {
  background-color: #6ec563;  /* for browsers that don't get gradients */
  background: -moz-linear-gradient(center top, rgba(110, 197, 99, 0.9), rgba(110, 197, 99, 0.5) 35%, #6EC563 75%, #6EC563);
  background: -webkit-gradient(linear, 0% 0%, 0% 100%, from(rgba(110, 197, 99, 0.9)), to(#6EC563), color-stop(0.35, rgba(110, 197, 99, 0.5)), color-stop(0.75, #6EC563));
}

.token_results {
  margin-top: 2em;
}

#products {
  margin-top: 1em;
}

#products .columns {
  -moz-column-count: 2;
  -moz-column-gap: 30px;
  -webkit-column-count: 2;
  -webkit-column-gap: 30px;
  column-count: 2;
  column-gap: 30px;
}

.product_count {
  margin-bottom: 14px;
}

.product_count span {
  padding: 3px 8px 3px 20px;
  border: 1px solid #bbb;
  -moz-border-radius: 14px;
  -webkit-border-radius: 14px;
  border-radius: 14px;
  background: url(/images/manage/bullet_go.png) no-repeat 2px 55%;
  background-color: #f3f3f3; /* for browsers that don't get gradients */
  background-image: url(/images/manage/bullet_go.png), -moz-linear-gradient(top center, #eee, #fff 35%, #efefef 65%, #dedede);
  background-image: url(/images/manage/bullet_go.png), -webkit-gradient(linear, 0% 0%, 0% 100%, from(#eee), to(#dedede), color-stop(0.35, #fff), color-stop(0.65, #efefef));
}

#body-content #content {position:static;}

.left-text {text-align:left;}
